{
  "name": "@iconsans/react",
  "version": "0.1.0",
  "description": "+320 Free Icons for your next project",
  "keywords": [],
  "author": {
    "name": "Morteza Sabihi",
    "email": "morteza.sabihi@gmail.com",
    "url": "https://mortezasabihi.com"
  },
  "license": "MIT",
  "main": "./dist/packages/react/index.cjs",
  "module": "./dist/packages/react/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/packages/react/index.js",
      "require": "./dist/packages/react/index.cjs"
    }
  },
  "directories": {
    "componentns": "componentns"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mortezasabihi/iconsans.git"
  },
  "scripts": {
    "preconvert": "rimraf components && mkdir components",
    "convert": "npx @svgr/cli --typescript --replace-attr-values '#00153F=currentColor' --filename-case 'kebab' --no-dimensions --ref --out-dir ./components --ignore-existing -- ../../icons",
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && vite build"
  },
  "bugs": {
    "url": "https://github.com/mortezasabihi/iconsans/issues"
  },
  "homepage": "https://github.com/mortezasabihi/iconsans#readme",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^7.0.0",
    "@types/react": "^18.0.38",
    "@vitejs/plugin-react": "^4.0.0"
  }
}
